<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>http://myfaces.apache.org/sandbox</namespace>

    <tag>
        <tag-name>selectItems</tag-name>
        <component>
            <component-type>org.apache.myfaces.UISelectItems</component-type>
        </component>
    </tag>
    
    <tag>
        <tag-name>filterTable</tag-name>
        <component>
            <component-type>org.apache.myfaces.FilterTable</component-type>
            <renderer-type>org.apache.myfaces.FilterTable</renderer-type>
        </component>
    </tag>
    
</facelet-taglib>